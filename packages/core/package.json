{
  "name": "@wire-dsl/core",
  "version": "0.1.2",
  "description": "Core WireDSL engine - Parser, IR generator, layout engine, and SVG renderer",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "wireframe",
    "dsl",
    "declarative",
    "design",
    "prototype",
    "svg"
  ],
  "author": "WireDSL Contributors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chevrotain": "11.1.0",
    "zod": "4.3.5",
    "pdfkit": "0.17.2",
    "sharp": "0.34.5",
    "svg-to-pdfkit": "0.1.8"
  },
  "devDependencies": {
    "@types/node": "25.0.9",
    "@typescript-eslint/eslint-plugin": "8.53.1",
    "@typescript-eslint/parser": "8.53.1",
    "eslint": "9.39.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.17"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
