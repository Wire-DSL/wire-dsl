{
  "name": "@wire-dsl/cli",
  "version": "0.0.1",
  "description": "WireDSL CLI - Command-line tool for wireframe generation and validation",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "wire": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup src/index.ts src/cli.ts --format cjs,esm --dts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "wireframe",
    "cli",
    "command-line",
    "dsl"
  ],
  "author": "WireDSL Contributors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@wire-dsl/core": "workspace:*",
    "chokidar": "5.0.0",
    "chalk": "5.6.2",
    "commander": "14.0.2",
    "ora": "9.1.0",
    "sharp": "0.34.5"
  },
  "devDependencies": {
    "@types/node": "25.0.9",
    "@typescript-eslint/eslint-plugin": "8.53.1",
    "@typescript-eslint/parser": "8.53.1",
    "eslint": "9.39.2",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vitest": "4.0.17"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
