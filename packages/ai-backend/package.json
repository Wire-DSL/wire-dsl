{
  "name": "@wire-dsl/ai-backend",
  "version": "0.0.1",
  "description": "WireDSL AI Backend - Cloudflare Workers service for LLM integration",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node --external:@cloudflare/workers-types",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "ai",
    "llm",
    "wireframe"
  ],
  "author": "WireDSL Contributors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "hono": "^3.11.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231121.0",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "wrangler": "^3.25.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
