{
  "name": "@wire-dsl/ai-backend",
  "version": "0.0.1",
  "description": "WireDSL AI Backend - Cloudflare Workers service for LLM integration",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "build": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=node --external:@cloudflare/workers-types",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix"
  },
  "keywords": [
    "cloudflare",
    "workers",
    "ai",
    "llm",
    "wireframe"
  ],
  "author": "WireDSL Contributors",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "hono": "4.11.4",
    "zod": "4.3.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20260120.0",
    "@types/node": "25.0.9",
    "@typescript-eslint/eslint-plugin": "8.53.1",
    "@typescript-eslint/parser": "8.53.1",
    "esbuild": "0.27.2",
    "eslint": "9.39.2",
    "typescript": "5.9.3",
    "wrangler": "4.59.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
