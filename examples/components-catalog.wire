            component Select label: "Country" items: "USA,Canada,Mexico,UK,France,Germany,Spain,Italy"
            component Select label: "User Role" items: "Admin,Editor,Viewer,Guest"
            component Select label: "Account Status" items: "Active,Inactive,Pending,Archived"
            component Select label: "Priority Level" items: "Low,Medium,High,Critical"
          }
        }
      }

      layout card(padding: md) {
        component Heading text: "Boolean Input Types" level: h3

        layout stack(direction: vertical, gap: sm) {
          component Text content: "Checkboxes (multi-select):"
          component Checkbox label: "I agree to the terms and conditions" checked: true
          component Checkbox label: "Subscribe to newsletter" checked: false
          component Checkbox label: "Remember my login" checked: true
