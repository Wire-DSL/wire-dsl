project "Wire-DSL Complete Components Catalog" {
  style {
    device: "desktop"
    density: "normal"
    spacing: "md"
    radius: "md"
    stroke: "normal"
    font: "base"
  }

  colors {
    accent: #007BFF
    control: #28A745
    chart: #17A2B8
  }

  screen ComponentsCatalog {
    layout stack(direction: vertical, gap: lg, padding: lg) {

      component Heading text: "Wire-DSL Components Catalog"
      component Text content: "Comprehensive showcase of all 28 components and 5 container types. This catalog demonstrates every variant, property combination, and container layout available in Wire-DSL for reference and testing."

      component Divider

      component Heading text: "1. Text Components" level: h2
      component Text content: "Typography elements for displaying text at different hierarchy levels."

      layout card(padding: md) {
        component Heading text: "Heading - H1 Level (Largest)" level: h1
        component Heading text: "Heading - H2 Level (Section Header)" level: h2
        component Heading text: "Heading - H3 Level (Subsection)" level: h3
        component Text content: "Text component for body content. This is the standard text element used for paragraphs and descriptions. It automatically wraps across multiple lines when content exceeds the available width."
        component Label text: "Label - Small form label text"
        component Label text: "Email Address"
        component Label text: "Password Required *"
      }

      component Divider

      component Heading text: "2. Input Components" level: h2
      component Text content: "Form input elements for collecting user data with validation and interaction."

      layout grid(columns: 2, gap: md) {
        cell span: 1 {
          layout card(padding: md) {
            component Heading text: "Text Inputs" level: h3
            component Input label: "Email" placeholder: "you@example.com"
            component Input label: "Search Query" placeholder: "Type to search..."
